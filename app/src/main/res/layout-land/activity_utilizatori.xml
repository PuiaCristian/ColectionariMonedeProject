<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_utilizatori"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:baselineAligned="false"
    android:animateLayoutChanges="true"
    android:orientation="vertical"
    tools:context="com.example.puya_.colectionarimonedeproject.Utilizatori">

    <fragment
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/fragment_detalii_utilizatori"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        class="com.example.puya_.colectionarimonedeproject.DetaliiUtilizatoriFragment"/>

    <fragment
        android:layout_width="150dp"
        android:layout_height="match_parent"
        android:id="@+id/fragment_lista_utilizatori"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        class="com.example.puya_.colectionarimonedeproject.ListaUtilizatoriFragment"/>

</RelativeLayout>
